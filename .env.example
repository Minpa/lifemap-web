# CloudKit Configuration
# Get these values from Apple Developer Portal > CloudKit Dashboard
NEXT_PUBLIC_CLOUDKIT_CONTAINER_ID=iCloud.com.yourcompany.lifemap
NEXT_PUBLIC_CLOUDKIT_API_TOKEN=your_cloudkit_api_token_here
NEXT_PUBLIC_CLOUDKIT_ENVIRONMENT=development

# Mapbox Configuration
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Database Configuration (for sharing feature)
DATABASE_URL=postgresql://user:password@host:port/database

# NextAuth Configuration (if using NextAuth)
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000
